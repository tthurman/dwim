
buildscript {
   
    repositories {
	mavenCentral()
        jcenter()
    }

    dependencies {
        classpath 'com.android.tools.build:gradle:1.1.3'
    }
}

apply plugin: 'com.android.application'

dependencies {
    compile 'org.jsoup:jsoup:1.9.2'
}

repositories {
    mavenCentral()
    jcenter()
}

android {
    compileSdkVersion 22
    buildToolsVersion "22.0.1"
}

task wrapper(type: Wrapper) {
    gradleVersion = '2.3'
}

